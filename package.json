{
  "name": "@ntlab/identity-authenticator",
  "version": "1.0.0",
  "description": "An identity authentication using fingerprints or faces",
  "main": "app.js",
  "scripts": {
    "idserver": "node app.js --port=7978",
    "idproxy": "node app.js --port=7978 --config=proxy.json",
    "idworker1": "node app.js --port=8001",
    "idworker2": "node app.js --port=8002",
    "build:dplib": "cd node_modules/@ntlab/dplib && node-gyp rebuild",
    "build:dplib:win32": "cd node_modules/@ntlab/dplib && node-gyp rebuild --arch=ia32",
    "build:dplib:win64": "cd node_modules/@ntlab/dplib && node-gyp rebuild --arch=x64",
    "build:opencv": "cd node_modules/@u4/opencv4nodejs && node-gyp rebuild",
    "build:opencv:win32": "set OPENCV4NODEJS_INCLUDES=../../../opencv/include&& set OPENCV4NODEJS_LIBRARIES=../../../../opencv/x86/vc17/lib/opencv_world480.lib;../../../../opencv/x86/vc17/lib/opencv_img_hash480.lib&&cd node_modules/@u4/opencv4nodejs && node-gyp rebuild --arch=ia32",
    "build:opencv:win64": "set OPENCV4NODEJS_INCLUDES=../../../opencv/include&& set OPENCV4NODEJS_LIBRARIES=../../../../opencv/x64/vc17/lib/opencv_world480.lib;../../../../opencv/x64/vc17/lib/opencv_img_hash480.lib&&cd node_modules/@u4/opencv4nodejs && node-gyp rebuild --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tohenk/node-identity-authenticator.git"
  },
  "keywords": [
    "fingerprint",
    "face",
    "identify",
    "authenticator",
    "digital persona",
    "opencv"
  ],
  "author": "Toha <tohenk@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tohenk/node-identity-authenticator/issues"
  },
  "homepage": "https://github.com/tohenk/node-identity-authenticator#readme",
  "dependencies": {
    "@ntlab/identity-face": "^1.0.1",
    "@ntlab/identity-fingerprint": "^1.0.1",
    "@ntlab/ntlib": "^2.0.5",
    "@ntlab/work": "^1.1.3"
  }
}
